{
  "name": "spotify-wrapper",
  "version": "1.0.0",
  "description": "Wrapper for API Spotify",
  "main": "src/index.js",
  "scripts": {
    "clear": "rimraf lib",
    "build": "npm run clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "build:umd": "./node_modules/.bin/webpack --output-filename spotify-wrapper.umd.js",
    "build:umd:min": "./node_modules/.bin/webpack --output-filename spotify-wrapper.umd.min.js -p",
    "build:all": "npm run build && npm run build:umd && npm run build:umd:min",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HigorMonteiro/spotify-wrapper.git"
  },
  "author": "Higor Vinicius",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HigorMonteiro/spotify-wrapper/issues"
  },
  "homepage": "https://github.com/HigorMonteiro/spotify-wrapper#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.2",
    "json-loader": "^0.5.7",
    "mocha": "^5.2.0",
    "node-fetch": "^2.2.0",
    "sinon": "^6.1.3",
    "sinon-chai": "^3.2.0",
    "webpack": "^2.7.0",
    "webpack-cli": "^3.1.2"
  }
}
